{
  "compilerOptions": {
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "noImplicitAny": false,
      "jsx": "react",
      "resolveJsonModule": true,
      "noLib": false,
      "suppressImplicitAnyIndexErrors": true,
      "baseUrl": ".",
      "declaration": true,
      "experimentalDecorators": true,
      "lib": [
          "es6",
          "dom"
      ],
      "outDir": "dist",
      "target": "es5",
      "types": [
          "cypress",
          "node"
      ],
      "sourceMap": false
  },
  "include": [
      "cypress/integration",
      "cypress/fixtures",
      "cypress/page-objects",
      "cypress/plugins",
      "cypress/support"
  ],
  "compileOnSave": false,
  "exclude": [
      "node_modules",
      "cypress/reports",
      "logs",
      "*.test.js",
      "*.test.jsx",
      "*.test.ts",
      "*.test.tsx",
      "*.spec.js",
      "*.spec.jsx",
      "*.spec.ts",
      "*.spec.tsx"
  ]
}
